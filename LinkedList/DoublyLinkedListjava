public class DoublyLinkedList {
    public class  Node {
        int data;
        Node Next;
        Node Prev;
        
        public Node(int data) {
            this.data = data;
            this.Next = null;
            this.Prev = null;

        }
    }
    
    public static Node head;
    public static Node tail;
    public static int size;
}

public static void main(String[] args) {

}